<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for prepress.proof -->
        <record id="seq_prepress_proof" model="ir.sequence">
            <field name="name">Prepress proof</field>
            <field name="code">prepress.proof</field>
            <field name="prefix">BT</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for prepress.cutting.die -->
        <record id="seq_prepress_cutting_die" model="ir.sequence">
            <field name="name">Prepress Cutting die</field>
            <field name="code">prepress.cutting.die</field>
            <field name="prefix">FD</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for prepress.plate -->
        <record id="seq_prepress_plate" model="ir.sequence">
            <field name="name">Prepress Plate</field>
            <field name="code">prepress.plate</field>
            <field name="prefix">P</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
